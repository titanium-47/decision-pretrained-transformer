{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug train_interactive (bandit)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/train_interactive.py",
      "args": [
        "--env",
        "bandit",
        "--envs",
        "100000",
        "--H",
        "500",
        "--dim",
        "5",
        "--var",
        "0.3",
        "--cov",
        "0.0",
