    {
      "name": "Debug eval (bandit)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/eval.py",
      "args": [
        "--env",
        "bandit",
        "--envs",
        "100",
        "--H",
        "500",
        "--dim",
        "5",
        "--var",
        "0.3",
        "--cov",
        "0.0",
        "--lr",
        "0.0001",
        "--layer",
        "4",
        "--head",
        "4",
        "--shuffle",
        "--epoch",
        "50",
        "--n_eval",
        "200",
        "--seed",
        "1",
